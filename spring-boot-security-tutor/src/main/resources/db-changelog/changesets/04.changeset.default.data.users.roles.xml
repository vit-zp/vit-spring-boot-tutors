<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog/1.7"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog/1.7
         http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-1.7.xsd">
         <changeSet id="04.changeset.insert.role.admin" author="Vit.Khodak" failOnError="false" dbms="h2">
        	<insert tableName="Roles" >
        		<column name="ROLE_NAME" value="ROLE_ADMIN"></column>
        	</insert>
    </changeSet>
    
    <changeSet id="04.changeset.insert.user.admin" author="Vit.Khodak" failOnError="false" dbms="h2">
        	<insert tableName="Users" >
        		<column name="USER_NAME" value="admin"></column>
        		<column name="ENCRYPTED_PASSWORD" value="0000"></column>
        		<column name="ENABLED" value="true"></column>
        	</insert>
    </changeSet>
</databaseChangeLog>